<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_gravity="center_horizontal"
    android:layout_height="wrap_content"
    android:background="@color/colorWhite">

    <ImageView
        android:id="@+id/activity_result_tracker_item_image_view2"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:paddingTop="5dp"
        android:src="@drawable/result_tracker_ic_gpa"
        android:tint="@color/colorVividBlue"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/gpaText2"
        app:layout_constraintEnd_toStartOf="@+id/gpaText2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gpaText2" />

    <TextView
        android:id="@+id/gpaText2"
        android:layout_width="163dp"
        android:layout_height="26dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/montserrat_semi_bold"
        android:text="@string/activityResultTrackerCGPA"
        android:textColor="@color/colorVividBlue"
        android:textSize="@dimen/EditTextTextSizeLarge"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/activity_result_tracker_item_image_view2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view11"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorVividBlue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gpaText2" />
</androidx.constraintlayout.widget.ConstraintLayout>