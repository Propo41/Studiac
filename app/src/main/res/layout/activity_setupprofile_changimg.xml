<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorVividBlue"
    tools:context=".SetupprofileChangimg">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/changimgtext"
        android:textColor="@color/colorWhite"
        android:textSize="25dp"
        android:fontFamily="@font/montserratsemibold"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        />
    <ImageView
        android:id="@+id/change_img1_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="80dp"
        />
    <ImageView
        android:id="@+id/change_img2_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"

        android:layout_marginTop="80dp"
        android:layout_toRightOf="@+id/change_img1_id"
        />
    <ImageView
        android:id="@+id/change_img3_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"
        android:layout_marginTop="80dp"
        android:layout_toRightOf="@+id/change_img2_id"
        />

    <ImageView
        android:id="@+id/change_img4_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="210dp"
        />
    <ImageView
        android:id="@+id/change_img5_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"
        android:layout_marginTop="210dp"
        android:layout_toRightOf="@+id/change_img4_id"
        />
    <ImageView
        android:id="@+id/change_img6_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"
        android:layout_marginTop="210dp"
        android:layout_toRightOf="@+id/change_img5_id"
        />

    <ImageView
        android:id="@+id/change_img7_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="340dp"
        />
    <ImageView
        android:id="@+id/change_img8_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"

        android:layout_marginTop="340dp"
        android:layout_toRightOf="@+id/change_img7_id"
        />
    <ImageView
        android:id="@+id/change_img9_id"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:src="@drawable/profile"

        android:layout_marginTop="340dp"
        android:layout_toRightOf="@+id/change_img8_id"
        />
    <Button
        android:id="@+id/changeimg_button_id"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/custom_button"
        android:layout_marginTop="510dp"
        android:text="@string/changimgbuttontext"
        android:textColor="@color/colorVividBlue"
        android:fontFamily="@font/montserratmedium"
        android:textSize="12dp"
        android:textAlignment="center"
        android:elevation="10dp"

        />


</RelativeLayout>