<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorVividBlue"
    android:gravity="center"
    tools:context=".activities.setup.SelectImageActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="@string/SelectImage_text"
        android:textColor="@color/colorWhite"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/profile_picture_three_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_three"
        android:textOff=""
        android:layout_marginEnd="5dp"
        android:textOn=""
        app:layout_constraintBottom_toTopOf="@+id/profile_picture_one_id"
        app:layout_constraintEnd_toStartOf="@+id/profile_picture_four_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline7"
        app:layout_constraintVertical_chainStyle="packed" />

    <ToggleButton
        android:id="@+id/profile_picture_four_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_four"
        android:layout_marginEnd="5dp"

        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_three_id"
        app:layout_constraintEnd_toStartOf="@+id/profile_picture_five_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile_picture_three_id"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_three_id" />


    <ToggleButton
        android:id="@+id/profile_picture_one_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:layout_marginTop="10dp"
        android:background="@drawable/profile_picture_one"
        android:layout_marginEnd="5dp"

        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toTopOf="@+id/profile_picture_six_id"
        app:layout_constraintEnd_toStartOf="@+id/profile_picture_seven_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_picture_three_id" />

    <ToggleButton
        android:id="@+id/profile_picture_two_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_two"
        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_seven_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile_picture_seven_id"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_seven_id" />


    <ToggleButton
        android:id="@+id/profile_picture_five_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_five"


        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_four_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile_picture_four_id"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_four_id" />

    <ToggleButton
        android:id="@+id/profile_picture_seven_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_seven"
        android:layout_marginEnd="5dp"

        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_one_id"
        app:layout_constraintEnd_toStartOf="@+id/profile_picture_two_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile_picture_one_id"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_one_id" />

    <ToggleButton
        android:id="@+id/profile_picture_eight_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_eight"
        android:textOff=""
        android:layout_marginEnd="5dp"

        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_six_id"
        app:layout_constraintEnd_toStartOf="@+id/profile_picture_nine_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile_picture_six_id"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_six_id" />

    <ToggleButton
        android:id="@+id/profile_picture_six_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:layout_marginTop="10dp"
        android:background="@drawable/profile_picture_six"

        android:layout_marginEnd="5dp"

        android:textOff=""
        android:textOn=""
        app:layout_constraintEnd_toStartOf="@+id/profile_picture_eight_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_picture_one_id" />

    <ToggleButton
        android:id="@+id/profile_picture_nine_id"
        android:layout_width="@dimen/ImageExtraSizeLarge"
        android:layout_height="@dimen/ImageExtraSizeLarge"
        android:background="@drawable/profile_picture_nine"
        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_eight_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/profile_picture_eight_id"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_eight_id" />

    <Button
        android:id="@+id/changeimg_button_id"
        style="@style/EditTextShadowStyle"
        android:layout_width="0dp"
        android:layout_height="@dimen/ButtonHeightLarge"
        android:background="@drawable/ripple_effect_1"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/Next_Btn"
        android:textAlignment="center"
        android:textColor="@color/colorVividBlue"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/select_img_progress_bar"
        app:layout_constraintEnd_toEndOf="@+id/profile_picture_nine_id"
        app:layout_constraintStart_toStartOf="@+id/profile_picture_six_id"
       />

    <ProgressBar
        android:id="@+id/select_img_progress_bar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:indeterminate="true"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15" />


</androidx.constraintlayout.widget.ConstraintLayout>